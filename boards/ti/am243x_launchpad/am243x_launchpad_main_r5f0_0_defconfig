CONFIG_SYS_CLOCK_HW_CYCLES_PER_SEC=25000000

# XIP is deactivated for now since we have no flash support yet
CONFIG_XIP=n

# Serial Driver
CONFIG_SERIAL=y

# Enable Console
CONFIG_CONSOLE=y
CONFIG_UART_CONSOLE=y
